{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["es2017", "dom", "ESNext"],
    "sourceMap": true,
    "target": "es5",
    "noUnusedLocals": true,
    "downlevelIteration": true,
//    "noImplicitAny": true,
//    "noImplicitThis": true,
//    "strictNullChecks": true,
    "types": ["webpack-env"],
    "paths": {
      "@temp/*": ["./src/*"],
      "@sdk/*": ["./src/@sdk/*"],
      "@images/*": ["./src/images/*"],
      "@locale/*": ["locale/*"],
      "@assets/*": ["assets/*"],
      "images/*": ["./src/images/*"]
    },
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
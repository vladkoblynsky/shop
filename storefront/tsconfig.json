{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "es2017",
      "dom",
      "ESNext"
    ],
    "sourceMap": true,
    "target": "es5",
    "noUnusedLocals": true,
    "downlevelIteration": true,
    //    "noImplicitAny": true,
    //    "noImplicitThis": true,
    //    "strictNullChecks": true,
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@temp/*": [
        "./src/*"
      ],
      "@app/*": [
        "./src/@next/*"
      ],
      "@sdk/*": [
        "./src/@sdk/*"
      ],
      "images/*": [
        "./src/images/*"
      ]
    },
    "skipLibCheck": true,
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.config.js"
  ]
}
